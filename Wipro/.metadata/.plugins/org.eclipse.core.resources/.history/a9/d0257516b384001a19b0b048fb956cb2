
import java.util.*;
 
public class Distance {
    @SuppressWarnings("resource")
	public static void main(String args[] ) throws Exception {
       
    	  Scanner sc=new Scanner(System.in);
	        int point1=sc.nextInt();
	        int point2=sc.nextInt();
	        int point3=sc.nextInt();
	        int point4=sc.nextInt();
	        
	        
	        Point pointx = new Point();
	        pointx.point1 = point1;
	        pointx.point2 = point3;
	        Point pointy = new Point();
	        pointy.point1 = point2;
	        pointy.point2 = point4;
	        findDistance(pointx, pointy);
         
}
 
 
  public static void findDistance(Point p1, Point p2)
         {
	      System.out.print(p1.point1);
     
         } 
   
 
static class Point
{
	public int point2;
	public int point1;
 
}
}
